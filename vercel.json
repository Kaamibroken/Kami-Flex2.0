{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "signin.html", "use": "@vercel/static" },
    { "src": "signup.html", "use": "@vercel/static" },
    { "src": "home.html", "use": "@vercel/static" },
    { "src": "data.js", "use": "@vercel/node" },
    { "src": "style.css", "use": "@vercel/static" },
    { "src": "images/(.*)", "use": "@vercel/static" },
    { "src": "database.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/signin", "dest": "/signin.html" },
    { "src": "/signup", "dest": "/signup.html" },
    { "src": "/home", "dest": "/home.html" },
    { "src": "/api/(.*)", "dest": "/data.js" },
    { "src": "/database", "dest": "database/data.html" }
  ]
}

